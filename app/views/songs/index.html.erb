<table>
  <tr>
    <th>Artist</th>
    <th>Title</th>
    <th>Version</th>
    <th>Tuning</th>
    <th>Capo</th>
    <th>Chords</th>
    <th>Parts</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @songs.each do |song| %>
  <tr>
    <td><%= song.artist %></td>
    <td><%= song.title %></td>
    <td><%= song.version %></td>
    <td><%= song.tuning %></td>
    <td><%= song.capo %></td>
    <td><%= song.chords %></td>
    <td><%= song.parts %></td>
    <td><%= link_to 'Show', song %></td>
    <td><%= link_to 'Edit', edit_song_path(song) %></td>
    <td><%= link_to 'Destroy', song, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>